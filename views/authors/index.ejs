<h2>Search Authors</h2>

<form action="/authors" method="get">
  <label for="">Name</label>
  <input type="text" name="name" value="<%= searchOptions.name %>" autocomplete="off">
  <button type="submit">Search</button>
</form>
<br />

<% authors.forEach(author => { %>
  <div><%= author.name %></div>
<% }) %>